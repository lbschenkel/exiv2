#: src/exiv2.cpp:235
msgid "Usage:"
msgstr "Uso:"

#: src/exiv2.cpp:236
msgid "[ options ] [ action ] file ...\n\n"
msgstr "[ opções ] [ ação ] arquivo ...\n\n"

#: src/exiv2.cpp:237
msgid "Manipulate the Exif metadata of images.\n"
msgstr "Manipule os metadados Exif das imagens.\n"

#: src/exiv2.cpp:262
msgid "\n"
"Actions:\n"
msgstr "\n"
"Ações:\n"

#: src/exiv2.cpp:263
msgid "  ad | adjust   Adjust Exif timestamps by the given time. This action\n"
"                requires at least one of the -a, -Y, -O or -D options.\n"
msgstr "  ajuste o horário Exif até o horário indicado. Esta ação\n"
"                requer pelo menos um dos -a, -Y, -O ou -D opções.\n"

#: src/exiv2.cpp:265
msgid "  pr | print    Print image metadata.\n"
msgstr "  pr | print    Print image metadata.\n"

#: src/exiv2.cpp:266
msgid "  rm | delete   Delete image metadata from the files.\n"
msgstr "  rm ➤ Deleta os metadados da imagem dos arquivos.\n"

#: src/exiv2.cpp:267
msgid "  in | insert   Insert metadata from corresponding *.exv files.\n"
"                Use option -S to change the suffix of the input files.\n"
msgstr "  no Insert Insert metadata dos arquivos *.exv correspondentes.\n"
"                Use a opção -S para alterar o sufixo dos arquivos de entrada.\n"

#: src/exiv2.cpp:269
msgid "  ex | extract  Extract metadata to *.exv, *.xmp and thumbnail image files.\n"
msgstr "  ex/extract metadados para arquivos de imagem *.exv, *.xmp e miniatura.\n"

#: src/exiv2.cpp:270
msgid "  mv | rename   Rename files and/or set file timestamps according to the\n"
"                Exif create timestamp. The filename format can be set with\n"
"                -r format, timestamp options are controlled with -t and -T.\n"
msgstr "  mv eine renomeie arquivos e/ou defina timestamps de acordo com a\n"
"                Exif create timestamp. Formato do nome do arquivo pode ser definido com o formato\n"
"                -r, as opções de horário são controladas com -t e -T.\n"

#: src/exiv2.cpp:273
msgid "  mo | modify   Apply commands to modify (add, set, delete) the Exif and\n"
"                IPTC metadata of image files or set the JPEG comment.\n"
"                Requires option -c, -m or -M.\n"
msgstr "  mo 「modifique os comandos Aplicar para modificar (adicionar, definir, excluir) os metadados Exif e\n"
"                IPTC de arquivos de imagem ou defina o comentário JPEG.\n"
"                Requer opção -c, -m ou -M.\n"

#: src/exiv2.cpp:276
msgid "  fi | fixiso   Copy ISO setting from the Nikon Makernote to the regular\n"
"                Exif tag.\n"
msgstr "  fi Copy ISO definição fixiso da definição de Nikon Makernote para a regular\n"
"                tag Exif\n"

#: src/exiv2.cpp:278
msgid "  fc | fixcom   Convert the UNICODE Exif user comment to UCS-2. Its current\n"
"                character encoding can be specified with the -n option.\n"
msgstr "  fc ├fixcom Converter o comentário do usuário UNICODE Exif em UCS-2. Sua atual codificação de caracteres\n"
"                pode ser especificada com a opção -n.\n"

#: src/exiv2.cpp:280
msgid "\n"
"Options:\n"
msgstr "\n"
"Opções:\n"

#: src/exiv2.cpp:281
msgid "   -h      Display this help and exit.\n"
msgstr "   -h Exibe esta ajuda e sair.\n"

#: src/exiv2.cpp:282
msgid "   -V      Show the program version and exit.\n"
msgstr "   -V Mostra a versão e o fechamento do programa.\n"

#: src/exiv2.cpp:283
msgid "   -v      Be verbose during the program run.\n"
msgstr "   -v Seja detalhado durante a execução do programa.\n"

#: src/exiv2.cpp:284
msgid "   -q      Silence warnings and error messages during the program run (quiet).\n"
msgstr "   -q Silenciar avisos e mensagens de erro durante o programa executar (silencioso)\n"

#: src/exiv2.cpp:285
msgid "   -Q lvl  Set log-level to d(ebug), i(nfo), w(arning), e(rror) or m(ute).\n"
msgstr "   -Q Nível de log definido para d(ebug), i(nfo), w(arning), e(rror) ou m(ute).\n"

#: src/exiv2.cpp:286
msgid "   -b      Show large binary values.\n"
msgstr "   -b Mostrar grandes valores binários.\n"

#: src/exiv2.cpp:287
msgid "   -u      Show unknown tags.\n"
msgstr "   -u Mostra etiqueta desconhecidos.\n"

#: src/exiv2.cpp:288
msgid "   -g key  Only output info for this key (grep).\n"
msgstr "   -g key Somente informação de saída para essa chave (grep).\n"

#: src/exiv2.cpp:289
msgid "   -K key  Only output info for this key (exact match).\n"
msgstr "   -K apenas informações de saída da chave para esta chave (correspondência exata).\n"

#: src/exiv2.cpp:290
msgid "   -n enc  Charset to use to decode UNICODE Exif user comments.\n"
msgstr "   -n enc Conjunto de caracteres usado para decodificar comentários do usuário em UNICODE Exif.\n"

#: src/exiv2.cpp:291
msgid "   -k      Preserve file timestamps (keep).\n"
msgstr "   -k Preserve arquivo timestamps (manter).\n"

#: src/exiv2.cpp:292
msgid "   -t      Also set the file timestamp in 'rename' action (overrides -k).\n"
msgstr "   -t Também define o timestamp do arquivo na ação 'renomear' (substitui -k).\n"

#: src/exiv2.cpp:293
msgid "   -T      Only set the file timestamp in 'rename' action, do not rename\n"
"           the file (overrides -k).\n"
msgstr "   -T Apenas definir o timestamp do arquivo na ação 'renomear', não renomear\n"
"           o arquivo (substitui -k).\n"

#: src/exiv2.cpp:295
msgid "   -f      Do not prompt before overwriting existing files (force).\n"
msgstr "   -f Não pergunta antes de substituir arquivos existentes (força).\n"

#: src/exiv2.cpp:296
msgid "   -F      Do not prompt before renaming files (Force).\n"
msgstr "   -F Não pergunta antes de renomear arquivos (Força).\n"

#: src/exiv2.cpp:297
msgid "   -a time Time adjustment in the format [-]HH[:MM[:SS]]. This option\n"
"           is only used with the 'adjust' action.\n"
msgstr "   -Tempo ajuste no formato [-]H[:MM[:SS]]. Esta opção\n"
"           só é usada com a ação 'ajustar'.\n"

#: src/exiv2.cpp:299
msgid "   -Y yrs  Year adjustment with the 'adjust' action.\n"
msgstr "   -Y anos por ajuste com a ação 'ajuste'.\n"

#: src/exiv2.cpp:300
msgid "   -O mon  Month adjustment with the 'adjust' action.\n"
msgstr "   -O ajuste do mês mês com a ação 'ajuste'.\n"

#: src/exiv2.cpp:301
msgid "   -D day  Day adjustment with the 'adjust' action.\n"
msgstr "   -D Dia com a ação 'ajuste de'.\n"

#: src/exiv2.cpp:302
msgid "   -p mode Print mode for the 'print' action. Possible modes are:\n"
msgstr "   -p Modo de impressão para a ação 'print'. Possíveis modos são:\n"

