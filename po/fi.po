#: src/exiv2.cpp:235
msgid "Usage:"
msgstr "Käyttö:"

#: src/exiv2.cpp:236
msgid "[ options ] [ action ] file ...\n\n"
msgstr "[ options ] tiedosto ...\n\n"

#: src/exiv2.cpp:237
msgid "Manipulate the Exif metadata of images.\n"
msgstr "Manipuloi kuvien exif- metatiedot.\n"

#: src/exiv2.cpp:262
msgid "\n"
"Actions:\n"
msgstr "\n"
"Toimet:\n"

#: src/exiv2.cpp:263
msgid "  ad | adjust   Adjust Exif timestamps by the given time. This action\n"
"                requires at least one of the -a, -Y, -O or -D options.\n"
msgstr "  ad - säädä Säädä Exif aikaleimat annetun ajan mukaan. Tämä toiminto\n"
"                vaatii vähintään yhden -a, -Y, -O tai -D vaihtoehtoja.\n"

#: src/exiv2.cpp:265
msgid "  pr | print    Print image metadata.\n"
msgstr "  pr | print    Print image metadata.\n"

#: src/exiv2.cpp:266
msgid "  rm | delete   Delete image metadata from the files.\n"
msgstr "  rm − poista Poista kuvan metatiedot tiedostoista.\n"

#: src/exiv2.cpp:267
msgid "  in | insert   Insert metadata from corresponding *.exv files.\n"
"                Use option -S to change the suffix of the input files.\n"
msgstr "  lisää: Lisää Lisää metatiedot vastaavista *.exv-tiedostoista.\n"
"                Käytä vaihtoehtoa -S muuttaaksesi syöttötiedostojen jälkiliitettä.\n"

#: src/exiv2.cpp:269
msgid "  ex | extract  Extract metadata to *.exv, *.xmp and thumbnail image files.\n"
msgstr "  ex ¶ extract metatiedot to *.exv, *.xmp and thumbnail image files.\n"

#: src/exiv2.cpp:270
msgid "  mv | rename   Rename files and/or set file timestamps according to the\n"
"                Exif create timestamp. The filename format can be set with\n"
"                -r format, timestamp options are controlled with -t and -T.\n"
msgstr "  mv ¶ Nimeä tiedostot uudelleen ja/tai aseta tiedostojen aikaleimia\n"
"                Exif luo aikaleiman. Tiedostonimen muoto voidaan asettaa\n"
"                -r formaatilla, aikaleima vaihtoehtoja hallitaan -t ja -T.\n"

#: src/exiv2.cpp:273
msgid "  mo | modify   Apply commands to modify (add, set, delete) the Exif and\n"
"                IPTC metadata of image files or set the JPEG comment.\n"
"                Requires option -c, -m or -M.\n"
