msgstr "Valinnainen PLUS-tunnus, joka tunnistaa jokaisen lisenssin."

#: src/properties.cpp:1028
msgid "Licensee Name"
msgstr "Lisenssinviejän Nimi"

#: src/properties.cpp:1028
msgid "Name of each Licensee."
msgstr "Kunkin lisenssin nimi"

#: src/properties.cpp:1029
msgid "End User"
msgstr "Lopeta Käyttäjä"

#: src/properties.cpp:1029
msgid "Party or parties ultimately making use of the image under the license."
msgstr "Puolue tai osapuolet hyödyntävät viime kädessä kuvan nojalla lisenssin."

#: src/properties.cpp:1030
msgid "End User ID"
msgstr "Loppukäyttäjän Tunnus"

#: src/properties.cpp:1030
msgid "Optional PLUS-ID identifying each End User."
msgstr "Valinnainen PLUS-ID kunkin loppukäyttäjän tunnistamiseksi."

#: src/properties.cpp:1031
msgid "End User Name"
msgstr "Loppukäyttäjän Nimi"

#: src/properties.cpp:1031
msgid "Name of each End User."
msgstr "Kunkin loppukäyttäjän nimi."

#: src/properties.cpp:1032
msgid "Licensor"
msgstr "Lisenssinantaja"

#: src/properties.cpp:1032
msgid "Party or parties granting the license to the Licensee."
msgstr "Osapuoli tai osapuolet, jotka myöntävät lisenssin lisenssille."

#: src/properties.cpp:1033
msgid "Licensor ID"
msgstr "Lisenssinantajan ID"

#: src/properties.cpp:1033
msgid "Optional PLUS-ID identifying each Licensor."
msgstr "Valinnainen PLUS-tunnus, jolla yksilöidään jokainen lisenssinantaja."

#: src/properties.cpp:1034
msgid "Licensor Name"
msgstr "Lisenssinantajan Nimi"

#: src/properties.cpp:1034
msgid "Name of each Licensor."
msgstr "Kunkin lisenssinsaajan nimi."

#: src/properties.cpp:1035
msgid "Licensor Address"
msgstr "Lisenssinantajan Osoite"

#: src/properties.cpp:1035
msgid "Licensor street address."
msgstr "Lisenssinantajan osoite"

#: src/properties.cpp:1036
msgid "Licensor Address Detail"
msgstr "Lisenssinantajan Osoitteen Tiedot"

#: src/properties.cpp:1036
