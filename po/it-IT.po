#: src/exiv2.cpp:235
msgid "Usage:"
msgstr "Uso:"

#: src/exiv2.cpp:236
msgid "[ options ] [ action ] file ...\n\n"
msgstr "[ options ] [ action ] file ...\n\n"

#: src/exiv2.cpp:237
msgid "Manipulate the Exif metadata of images.\n"
msgstr "Manipola i metadati Exif delle immagini.\n"

#: src/exiv2.cpp:262
msgid "\n"
"Actions:\n"
msgstr "\n"
"Azioni:\n"

#: src/exiv2.cpp:263
msgid "  ad | adjust   Adjust Exif timestamps by the given time. This action\n"
"                requires at least one of the -a, -Y, -O or -D options.\n"
msgstr "  ad <unk> regolare Adjust Exif timestamps dal tempo dato. Questa azione\n"
"                richiede almeno una delle -a, -Y, -O o -D opzioni.\n"

#: src/exiv2.cpp:265
msgid "  pr | print    Print image metadata.\n"
msgstr "  pr | print    Print image metadata.\n"

#: src/exiv2.cpp:266
msgid "  rm | delete   Delete image metadata from the files.\n"
msgstr "  rm <unk> delete Elimina i metadati dell'immagine dai file.\n"

#: src/exiv2.cpp:267
msgid "  in | insert   Insert metadata from corresponding *.exv files.\n"
"                Use option -S to change the suffix of the input files.\n"
msgstr "  in <unk> insert Inserisci i metadati dai file *.exv corrispondenti.\n"
"                Usa l'opzione -S per cambiare il suffisso dei file di ingresso.\n"

#: src/exiv2.cpp:269
msgid "  ex | extract  Extract metadata to *.exv, *.xmp and thumbnail image files.\n"
msgstr "  ex <unk> extract Estrai i metadati in file *.exv, *.xmp e miniature.\n"

#: src/exiv2.cpp:270
msgid "  mv | rename   Rename files and/or set file timestamps according to the\n"
"                Exif create timestamp. The filename format can be set with\n"
"                -r format, timestamp options are controlled with -t and -T.\n"
msgstr "  mv <unk> rinomina Rinomina i file e/o imposta i timestamp dei file in base al timestamp\n"
"                Exif. Il formato del nome file può essere impostato con il formato\n"
"                -r, le opzioni di timestamp sono controllate con -t e -T.\n"

#: src/exiv2.cpp:273
msgid "  mo | modify   Apply commands to modify (add, set, delete) the Exif and\n"
"                IPTC metadata of image files or set the JPEG comment.\n"
"                Requires option -c, -m or -M.\n"
msgstr "  mo <unk> modify Applica comandi da modificare (aggiungere, impostare, elimina) i metadati Exif e\n"
"                IPTC dei file immagine o imposta il commento JPEG.\n"
"                Richiede l'opzione -c, -m o -M.\n"

#: src/exiv2.cpp:276
msgid "  fi | fixiso   Copy ISO setting from the Nikon Makernote to the regular\n"
"                Exif tag.\n"
msgstr "  fi <unk> fixiso Copia l'impostazione ISO dalla Nikon Makernote al normale tag\n"
"                Exif.\n"

#: src/exiv2.cpp:278
msgid "  fc | fixcom   Convert the UNICODE Exif user comment to UCS-2. Its current\n"
"                character encoding can be specified with the -n option.\n"
msgstr "  fc <unk> fixcom Converti il commento utente Exif UNICODE in UCS-2. La codifica dei caratteri\n"
"                corrente può essere specificata con l'opzione -n.\n"

#: src/exiv2.cpp:280
msgid "\n"
"Options:\n"
msgstr "\n"
"Opzioni:\n"

#: src/exiv2.cpp:281
msgid "   -h      Display this help and exit.\n"
msgstr "   -h Mostra questo aiuto ed uscita.\n"

#: src/exiv2.cpp:282
msgid "   -V      Show the program version and exit.\n"
msgstr "   -V Mostra la versione del programma ed esci.\n"

#: src/exiv2.cpp:283
msgid "   -v      Be verbose during the program run.\n"
msgstr "   -v Sii prolisso durante l'esecuzione del programma.\n"

#: src/exiv2.cpp:284
msgid "   -q      Silence warnings and error messages during the program run (quiet).\n"
msgstr "   -q Silenziare gli avvisi e i messaggi di errore durante l'esecuzione del programma (silenzioso).\n"

#: src/exiv2.cpp:285
msgid "   -Q lvl  Set log-level to d(ebug), i(nfo), w(arning), e(rror) or m(ute).\n"
msgstr "   -Q lvl Imposta log-level a d(ebug), i(nfo), w(arning), e(rror) o m(uto).\n"

#: src/exiv2.cpp:286
msgid "   -b      Show large binary values.\n"
msgstr "   -b Mostra grandi valori binari.\n"

#: src/exiv2.cpp:287
msgid "   -u      Show unknown tags.\n"
msgstr "   -u Mostra tag sconosciuti.\n"

#: src/exiv2.cpp:288
msgid "   -g key  Only output info for this key (grep).\n"
msgstr "   -g key Solo informazioni di output per questa chiave (grep).\n"

#: src/exiv2.cpp:289
msgid "   -K key  Only output info for this key (exact match).\n"
msgstr "   -K key Solo informazioni di output per questa chiave (corrispondenza esatta).\n"

#: src/exiv2.cpp:290
msgid "   -n enc  Charset to use to decode UNICODE Exif user comments.\n"
msgstr "   -n enc Charset da usare per decodificare i commenti dell'utente UNICODE Exif.\n"

#: src/exiv2.cpp:291
msgid "   -k      Preserve file timestamps (keep).\n"
msgstr "   -k Preserva i timestamp dei file (keep).\n"

#: src/exiv2.cpp:292
msgid "   -t      Also set the file timestamp in 'rename' action (overrides -k).\n"
msgstr "   -t Imposta anche il timestamp del file nell'azione 'rinomina' (sovrascrive -k).\n"

#: src/exiv2.cpp:293
msgid "   -T      Only set the file timestamp in 'rename' action, do not rename\n"
"           the file (overrides -k).\n"
msgstr "   -T Imposta solo il timestamp del file in 'rinomina', non rinominare\n"
"           il file (sovrascrive -k).\n"

#: src/exiv2.cpp:295
msgid "   -f      Do not prompt before overwriting existing files (force).\n"
msgstr "   -f Non chiedere prima di sovrascrivere i file esistenti (forza).\n"

#: src/exiv2.cpp:296
msgid "   -F      Do not prompt before renaming files (Force).\n"
msgstr "   -F Non chiedere prima di rinominare i file (Forza).\n"

#: src/exiv2.cpp:297
msgid "   -a time Time adjustment in the format [-]HH[:MM[:SS]]. This option\n"
"           is only used with the 'adjust' action.\n"
msgstr "   -a time Time adjustment in the format [-]HH[:MM[:SS]]. This option\n"
"           is used only with the 'adjust' action.\n"

#: src/exiv2.cpp:299
msgid "   -Y yrs  Year adjustment with the 'adjust' action.\n"
msgstr "   -Y yrs Aggiustamento anno con l'azione 'aggiustamento'.\n"

#: src/exiv2.cpp:300
msgid "   -O mon  Month adjustment with the 'adjust' action.\n"
msgstr "   -O regolazione del mese con l'azione 'regola'.\n"

#: src/exiv2.cpp:301
msgid "   -D day  Day adjustment with the 'adjust' action.\n"
msgstr "   -D giorno Giorno di regolazione con l'azione 'regolazione'.\n"

#: src/exiv2.cpp:302
msgid "   -p mode Print mode for the 'print' action. Possible modes are:\n"
