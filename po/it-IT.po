msgstr "             :basename:   - nome del file originale senza estensione\n"

#: src/exiv2.cpp:352
msgid "             :dirname:    - name of the directory holding the original file\n"
msgstr "             :dirname:    - nome della directory con il file originale\n"

#: src/exiv2.cpp:353
msgid "             :parentname: - name of parent directory\n"
msgstr "             :parentname: - nome della directory padre\n"

#: src/exiv2.cpp:354
msgid "           Default filename format is "
msgstr "           Il formato predefinito del nome file è "

#: src/exiv2.cpp:356
